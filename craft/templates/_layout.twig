<script src="/assets/js/general.js"></script>
<script>
    $('#{{ entry.navTitle|lower }}').addClass('active').siblings().removeClass('active');

    $('.menu').on('click', function() {
        $('.mobile-nav').toggleClass('mobile-nav-open', 350)
		$('.menu').fadeToggle();
		$('.menu-overlay').fadeToggle();
	})
	$('.mobile-about').on('click', function() {
	    $('.about-chevron').toggleClass('chevron-open', 350);
	    $('.mobile-about').toggleClass('mobile-about-open', 350)
	})
		$('.mobile-services').on('click', function() {
		    $('.mobile-chevron').toggleClass('mobile-chevron-open', 350);
            $('.mobile-services').toggleClass('mobile-services-open', 350);
		})
	$('.mobile-footer-services').on('click', function() {
	    $('.mobile-footer-services').toggleClass('mobile-footer-service-open', 350);
        $('.service-chevron').toggleClass('chevron-open')
	})

	$('#submit').on('click', function() {
	    $('#estimate-email').val($('#estimate-email1').val())
	})
	
	setTimeout(function () {
        $('#flash-message').fadeOut('slow');
    }, 4000);

</script>
